    <PackageTags>WebSocket</PackageTags>
    <PackageLicenseUrl>https://github.com/forestpeas/forestpeas.WebSocketClient/blob/develop/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

</Project>
