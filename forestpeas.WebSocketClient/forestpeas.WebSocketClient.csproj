    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>forestpeas</Authors>
    <Description>A simple WebSocket client implementation with asynchronous APIs.</Description>
    <Copyright>forestpeas</Copyright>
    <PackageProjectUrl>https://github.com/forestpeas/forestpeas.WebSocketClient</PackageProjectUrl>
